.table {
    border:1px solid #dadada;
    border-right: 0;
    border-bottom: 0;
    position: relative;
    width: 88vw;
    height: 88vw;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
}
.tr {
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-grow: 1;
    overflow: hidden;
}
.td {
    border-bottom: 1px solid #dadada;
    border-right: 1px solid #dadada;
    text-align: center;
    width:100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-grow: 1;
    flex-shrink: 1;
    overflow: hidden;
    font-size: small;
    z-index: 10;
}
.empty {
  border: none !important;
  z-index: 1 !important;
}
.luckybtn {
  position: absolute;
  width: 44vw;
  height: 42vw;
  overflow: hidden;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  opacity: 1;
}